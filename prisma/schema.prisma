// database  : https://data.heroku.com/datastores/2771c217-6ca9-49db-a338-2adcfd08e1ad#administration

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
